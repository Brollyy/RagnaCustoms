<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <atom:link href="https://ragnacustoms.com/rss.xml" rel="self" type="application/rss+xml"/>
        <title>RagnaCustoms.com</title>
        <link>
        https://ragnacustoms.com</link>
        <description>All our catalog</description>
        {% for song in songs %}
            <item>
                {# <id>{{ url('song_detail',{id:song.id}) }}</id> #}
                <title>{{ song.name }} - {{ song.authorName }} new custom song for #Ragnar√∂ck #VR</title>
                <guid>{{ url('song_detail',{id:song.id}) }}</guid>
                <description><![CDATA[New Song : {{ song.name }}<br/>{{ url('song_detail',{id:song.id}) }} ]]>
                </description>
                <link>{{ url('song_detail',{id:song.id}) }}</link>
                <image>
                    <url>https://ragnacustoms.com/covers/{{ song.id }}{{ song.coverImageExtension }}</url>
                </image>
                {# <url>{{ url('song_detail',{id:song.id}) }}</url> #}
                {# <link rel="alternate" type="text/html" href="{{ url('song_detail',{id:song.id}) }}"/> #}
                <pubDate>{{ song.createdAt.format('r') }}</pubDate>
            </item>
        {% endfor %}

    </channel>
</rss>