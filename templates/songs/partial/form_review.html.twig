<form data-url="{{ url("form_review_save",{id:song.id}) }}" class="form ajax-form">
    <!-- FORM ROW -->
    <div class="form-row">
        <!-- FORM ITEM -->
        <div class="form-item">
            <!-- FORM RATING WRAP -->
            <div class="form-rating-wrap">
                <label>Fun Factor:</label>
                {% set rating = vote.funFactor %}
                <input type="hidden" value="{{ rating }}" name="funFactor"/>
                <div class="rating-list form-rating" data-input-selector="funFactor">
                    {% for i in 0..4 %}
                        <div class="rating medium {% if rating > i %}filled{% endif %}">
                            <svg class="rating-icon medium icon-star">
                                <use xlink:href="#svg-star"></use>
                            </svg>
                        </div>
                    {% endfor %}
                </div>
                <!-- /RATING LIST -->
            </div>
            <!-- /FORM RATING WRAP -->
        </div>
        <!-- /FORM ITEM -->
    </div>
    <!-- /FORM ROW -->
