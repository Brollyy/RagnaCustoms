<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for song in songs %}
        <url>
            <loc>{{ url('home',{search:("artist:"~song.authorName)|url_encode}) }}</loc>
            <lastmod>{{ song.lastDateUpload.format('Y-m-d') }}</lastmod>
            <changefreq>monthly</changefreq>
            <priority>0.8</priority>
        </url>
    {% endfor %}
</urlset>