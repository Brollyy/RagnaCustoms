{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/doctrine-migrations-bundle": "^3.1.1",
        "easycorp/easyadmin-bundle": "^4.1",
        "erusev/parsedown": "^1.7",
        "gedmo/doctrine-extensions": "^3.6",
        "intervention/image": "^2.7",
        "knpuniversity/oauth2-client-bundle": "^2.10",
        "nelmio/api-doc-bundle": "^4.4",
        "php-ffmpeg/php-ffmpeg": "^1.0",
        "pkshetlie/pagination-bundle": "^2.0",
        "pkshetlie/phpfcrc": "dev-main",
        "sensio/framework-extra-bundle": "^6.2",
        "sentry/sentry-symfony": "^4.2",
        "stof/doctrine-extensions-bundle": "^1.6",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/maker-bundle": "^1.30",
        "symfony/monolog-bundle": "^3.7",
        "symfony/routing": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/twig-bundle": "^6.0",
        "symfony/ux-swup": "^2.0",
        "symfony/validator": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/yaml": "6.0.*",
        "symfonycasts/reset-password-bundle": "^1.5",
        "symfonycasts/verify-email-bundle": "^1.10",
        "tetranz/select2entity-bundle": "^3.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/markdown-extra": "^3.3",
        "twig/twig": "^2.12|^3.0",
        "visualappeal/matomo-php-api": "^1.6",
        "wohali/oauth2-discord-new": "^1.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "dbrekelmans/bdi": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.0.*",
        "symfony/css-selector": "6.0.*",
        "symfony/debug-bundle": "^6.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
